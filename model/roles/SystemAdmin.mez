@Role("System Admin")
persistent object SystemAdmin {

  @requiredFieldValidator("validator.required_field")
  string firstName;

  @requiredFieldValidator("validator.required_field")
  string lastName;

  @requiredFieldValidator("validator.required_field")
  string mobileNumber;

  string emailAddress;

  EN_SYSTEM_ADMIN_ROLE_NAME roleName;

  bool deleted;

}
